package result;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;
import java.sql.ResultSet;

public class ResultDAO {
	public ResultBean getResult(int hno)
	{
		Class.forName("oracle.jdbc.driver.OracleDriver");
		Connection conn=DriverManager.getConnection("jdbc:oracle:thin:@localhost:1521:SYSTEM","system","tiger");
		PreparedStatement pstmt=conn.prepareStatement("select * from results where hno=?");
		pstmt.setInt(1, hno);
		ResultSet rs=pstmt.executeQuery();
		if(rs.next())
		{
			
		}
		else
		{
			
		}
	}
}
