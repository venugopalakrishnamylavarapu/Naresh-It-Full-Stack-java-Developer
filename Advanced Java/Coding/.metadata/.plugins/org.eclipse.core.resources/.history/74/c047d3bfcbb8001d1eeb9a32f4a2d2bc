package com.TcsProbs;

import java.util.Scanner;

public class ReverseOfInteger {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int reverse=0;
		while(n!=0)
			if(n!=0)
			{
			int last=n%10;
			reverse=(reverse*10)+last;
			n/=10;
			}
		}
		System.out.println(reverse);
	}
}
