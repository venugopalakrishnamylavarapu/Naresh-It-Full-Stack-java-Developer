package com.TcsProbs;

import java.util.Scanner;

public class ReverseOfString {
	public static void main(String... heavens)
	{
		Scanner sc=new Scanner(System.in);
		String str=sc.nextLine();
		//the below logic is one of 3 basic logics to reverse a string
		//the remaining 2 are using StringBuilder class,itterating new String
		str=str.toLowerCase();
		StringBuffer sb=new StringBuffer();
		for(int i=str.length()-1;i>=0;i--)	sb.append(str.charAt(i));
		System.out.println(sb);
	}
}
