package com.TcsProbs;

import java.util.Scanner;

public class ReverseOfInteger {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int reverse=0;
		for(int i=0;i<=n;i++)
		{
			int last=n%10;
			reverse=(reverse*10)+last;
			n/=10;
		}
		System.out.println(reverse);
	}
}
